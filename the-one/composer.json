{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "repositories": [
      {
        "type": "vcs",
        "url": "http://gitlab.reclamare.ua/bower/bootstrap-fileinput.git"
      },
      {
        "type": "vcs",
        "url": "http://gitlab.reclamare.ua/composer/yii2-widget-fileinput.git"
      },
        {
            "type": "vcs",
            "url": "http://gitlab.reclamare.ua/common/the-one-html.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "symfony/var-dumper": "^2.7",
        "yiisoft/yii2-smarty": "^2.0",
        "2amigos/yii2-ckeditor-widget": "~1.0",
        "2amigos/yii2-file-upload-widget": "~1.0",
        "dmstr/yii2-adminlte-asset": "2.*",
        "reclamare/yii2-widget-fileinput": "~1.0",
        "zelenin/yii2-slug-behavior": "~1.0@stable",
        "kartik-v/yii2-widget-colorinput": "*",
        "yurkinx/yii2-image": "^1.1",
        "the-one/the-one-html":"dev-dev@dev",
        "yiisoft/yii2-jui": "^2.0",
        "sunra/php-simple-html-dom-parser": "^1.5",
        "zelenin/slugifier": "dev-master",
        "moonlandsoft/yii2-phpexcel": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800,
        "secure-http":false
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
